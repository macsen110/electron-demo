{
  "name": "electron-quick-start",
  "version": "9.1.0",
  "description": "A minimal Electron application",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "pack": "electron-builder --dir && build -mw",
    "dist": "electron-builder",
    "publish": "build --mac --win -p always",
    "win": "electron-packager . electron-quick-start --platform win32 --arch ia32 --out dist/ --ignore \"(dist|node_modules/electron.*)\" --overwrite"
  },
  "repository": "https://github.com/macsen110/electron-demo",
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial",
    "demo"
  ],
  "author": "Liangyusen",
  "license": "CC0-1.0",
  "devDependencies": {
    "electron": "^2.0.0",
    "electron-builder": "^20.24.5"
  },
  "build": {
    "appId": "org.develar.ElectronReact",
    "mac": {
      "category": "Development",
      "target": [
        "zip",
        "dmg"
      ]
    },
    "win": {
      "target": [
        {
          "target": "zip",
          "arch": [
            "x64",
            "ia32"
          ]
        }
      ],
      "publish": [
        "github"
      ]
    }
  },
  "dependencies": {
    "electron-is-dev": "^0.3.0",
    "electron-log": "^2.2.16",
    "electron-updater": "^3.0.3"
  }
}
